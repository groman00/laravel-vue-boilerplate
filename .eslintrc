{
  "extends": "airbnb-base",
  "plugins": ["vue"],
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
    "document": false,
    "navigator": false,
    "window": false
  },
  "rules": {
    "no-var": 0,
    "class-methods-use-this": 0,
    "object-shorthand": 0,
    "max-len": 0,
    "func-names": 0,
    "indent": [2, 4],
    "prefer-template": 0,
    "prefer-arrow-callback": 0,
    "no-param-reassign": 0,
    "no-unused-vars": [2, { "args": "none" }],
    "comma-dangle": 0,
    "no-console": 0,
    "quote-props": [2, "consistent"]
  }
}